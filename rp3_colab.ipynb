    {
      "cell_type": "code",
      "source": [
        "#@title Run the prediction\n",
        "scores_map = dict()\n",
        "for b in batches():\n",
        "    scores_map |= m.predict(b)\n"
      ],
      "metadata": {
        "id": "z2-PoDkqoVSc",
