    {
      "cell_type": "code",
      "source": [
        "#@title Print and save the results\n",
        "df = pd.DataFrame([[id, score] for (id, score) in scores_map.items()], columns=['id', 'score'])\n",
        "print(df)\n",
        "df.to_csv(\"rp3_scores.csv\", index=False)"
      ],
